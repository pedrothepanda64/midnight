{
	"name": "Midnight",
	"colors": {
		// ================ Base colors ================
		"icon.foreground": "#989898",

		// ================ Button control ================
		"checkbox.background": "#151515",
		"checkbox.foreground": "#0099ff", //"#007acc",
		"checkbox.border": "#ffffff00",

		// ================ Dropdown control ================
		"dropdown.background": "#151515",
		"dropdown.listBackground": "#151515",
		"dropdown.foreground": "#989898",

		// ================ Input control ================
		"input.background": "#151515",
		"input.foreground": "#989898",

		// ================ Activity bar ================
		"activityBar.background": "#0e0e0e",
		//"activityBar.foreground": "#007acc",
		"activityBar.inactiveForeground": "#989898",
		"activityBar.activeBackground": "#151515",
		"activityBar.activeBorder": "#007acc",
		"activityBarBadge.background": "#007acc", //"#0099ff",

		// ================ Side bar ================
		"sideBar.foreground": "#989898",
		"sideBar.background": "#0c0c0c",
		//"sideBar.border": "#252525",
		"sideBarTitle.foreground": "#bbbbbb",
		"sideBarSectionHeader.background": "#151515",
		"sideBarSectionHeader.foreground": "#bbbbbb",
		"sideBarSectionHeader.border": "#007acc",

		// ================ Editor groups & Tabs ================
		"editorGroupHeader.tabsBackground": "#080808",
		"tab.activeBackground": "#101010",
		"tab.activeBorderTop": "#007acc",
		"tab.inactiveBackground": "#020202",
		"tab.hoverBackground": "#ffffff01",

		// ================ Editor colors ================
		"editor.selectionBackground": "#3c3c3c",
		"editor.background": "#0c0c0c",
		"editor.foreground": "#989898", // Default text color
		"editor.selectionHighlightBackground": "#222222",
		"editor.selectionHighlightBorder": "#363636",
		"editorLineNumber.foreground": "#a0a0a0",
		"editorLineNumber.activeForeground": "#ffffff",
		"editor.lineHighlightBackground": "#1b1b1b",
		"editor.lineHighlightBorder": "#2a2a2a",
		"editorBracketMatch.border": "#ffff00",
		"editorGutter.addedBackground": "#3fb94f80", // editorGutter contains line numbers
		"editorGutter.deletedBackground": "#da363380",
		"editorGutter.commentRangeForeground": "#10a610",
		"editorGutter.background": "#111111", //"#323232",

		// ================ Panel ================
		"panel.background": "#0e0e0e",
		"panel.border": "#ffffff00",
		"panelTitle.activeBorder": "#007acc",
		//"panelSectionHeader.background": "#ff00ff", //"#080808", // What the fuck is this?

		// ================ Title bar ================
		"titleBar.activeBackground": "#101010",
		"titleBar.inactiveBackground": "#020202",
		//"titleBar.border": "#252525",

		// ================ Welcome page ================
		"welcomePage.progress.background": "#0c0c0c",
		"welcomePage.tileBackground": "#151515",
		"welcomePage.tileHoverBackground": "#202020",

		// ================ Settings editor ================
		"settings.modifiedItemIndicator": "#007acc",
		"settings.rowHoverBackground": "#ffffff02",
		"settings.focusedRowBackground": "#1b1b1b",
		"settings.focusedRowBorder": "#ffffff00"
	},

	// There's a bunch of stuff down here that I don't understand, and don't have time to mess with.
	// I've at least modified those that I actaully need, and I'll probably leave it at that.
	"tokenColors": [
		{
			"name": "Force Default", // Force certian elements to use the default text color.
			"scope": ["constant.numeric", "constant.hexadecimal"],
			"settings": {
				"foreground": "#989898"
			}
		},
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				//"fontStyle": "italic",
				"foreground": "#10a610"
			}
		},
		{
			"name": "Variables",
			"scope": ["variable.other", "variable.property"], //, "string constant.other.placeholder"],
			"settings": {
				"foreground": "#5b47ee" //"#EEFFFF"
			}
		},
		/*{
			"name": "Colors",
			"scope": ["constant.other.color"],
			"settings": {
				"foreground": "#ffffff"
			}
		},*/
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword.control",
				"keyword.other",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#5b47ee"
			}
		},
		{
			"name": "Preprocessor",
			"scope": ["keyword.control.include"],
			"settings": {
				"foreground": "#007bc8"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				//"keyword.control",
				"constant.other.color",
				//"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Constant",
			"scope": ["constant"],
			"settings": {
				"foreground": "#8b1e99"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#c8641e"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": ["meta.block variable.other"],
			"settings": {
				"foreground": "#5b47ee" //"#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": ["support.other.variable", "string.other.link"],
			"settings": {
				"foreground": "#f07178"
			}
		},
		/*{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
